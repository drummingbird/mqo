<openerp>
    <data>
	    <template id="index">
            <t t-call="website.layout">
                <t t-set="title">My learning</t>
                <div class="oe_structure">
                    <div class="container">
				        <t t-foreach="exercises" t-as="exercise">
				          <p><t t-esc="exercise.id"/> <t t-esc="exercise.name"/></p>
				          <p><t t-esc="exercise.instructions"/></p>
				          <t t-if="exercise.response_survey">
					          <p><t t-esc="exercise.response_survey.id"/></p>
					          <p><t t-esc="exercise.response_token"/></p>
					      </t>
				        </t>
				  	</div>
				</div>
			</t>    
	    </template>
    </data>
</openerp>